<resources>

    <!-- Base application theme. -->
    <style name="Theme.Feedback" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorSecondary</item>
        <item name="android:fontFamily">@font/roboto_thin</item>
    </style>
    <style name="Widget.Feedback.CustomRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/ic_star</item>
        <item name="android:indeterminateDrawable">@drawable/ic_star_outline</item>
    </style>
</resources>
